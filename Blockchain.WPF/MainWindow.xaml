<Window x:Class="Blockchain.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Blockchain.WPF"
        mc:Ignorable="d"
        Title="Project" Height="194.667" Width="426.333" FontFamily="Baskerville Old Face" FontSize="16">
    <Grid Background="{DynamicResource {x:Static SystemColors.DesktopBrushKey}}">
        <Button Name="ButtonUploadAccount" Content="Upload your Account" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{DynamicResource {x:Static SystemColors.DesktopBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontFamily="Baskerville Old Face" Width="166" Click="ButtonUploadAccount_Click" />
        <Button Name="ButtonCreateNewAccoutn"  Content="Create new Account" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Background="{DynamicResource {x:Static SystemColors.DesktopBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Width="166" FontFamily="Baskerville Old Face" Click="ButtonCreateNewAccoutn_Click"/>

    </Grid>
</Window>
